      "segment_id": "58cfcc64",
      "source_content": "---\ntitle: \"Grupy\"\nsidebar_label: Grupy\n---",
      "source_content_hash": "1bfdd99c36f45989d1f1df2473ab2966adf78a3024ae29b5b8204c6e809ab545",
      "node_type": "yaml",
      "translatable": false,
      "translations": {
